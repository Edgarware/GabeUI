    "version": "0.2.0",
    "configurations": [
      {
      "type": "lldb",
      "request": "launch",
      "name": "Launch",
      "args": [],
      "program": "${workspaceFolder}/target/debug/gabe_ui",
      "windows": {
          "program": "${workspaceFolder}/target/debug/gabe_ui.exe"
